{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ReverseProxy": {
    "Routes": {
      "ev-admin-route": {
        "ClusterId": "ev-admin-cluster",
        "Match": {
          "Path": "webApi/{**catch-all}"
        }
      },
      "ev-end-user-route": {
        "ClusterId": "ev-end-user-cluster",
        "Match": {
          "Path": "end-user-api/{**catch-all}"
        }
      }
    },
    "Clusters": {
      "ev-admin-cluster": {
        "Destinations": {
          "destination": {
            "Address": "https://localhost:9192"
          }
        }
      },
      "ev-end-user-cluster": {
        "Destinations": {
          "destination": {
            "Address": "https://localhost:9193"
          }
        }
      }
    }
  }
}